<h1>Foreign Agencies</h1>

<table width="100%" cellpadding="10">
  <thead>
    <tr>
      <th>Agency</th>
      <th>Region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agencies.each do |agency| %>
      <tr>
        <td width="30%" valign="top"><%= agency.title %></td>
        <td width="50%"><%= agency.region %></td>
        <td><%= link_to 'Edit', edit_agency_path(agency) %></td>
        <td><%= link_to 'Destroy', agency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agency', new_agency_path %>




