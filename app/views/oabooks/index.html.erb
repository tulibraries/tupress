<p id="notice"><%= notice %></p>

<h1>Oabooks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oabooks.each do |oabook| %>
      <tr>
        <td><%= link_to 'Show', oabook %></td>
        <td><%= link_to 'Edit', edit_oabook_path(oabook) %></td>
        <td><%= link_to 'Destroy', oabook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oabook', new_oabook_path %>
