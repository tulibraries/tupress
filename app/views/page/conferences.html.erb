
	<div class="row">
		<div class="col">
			<h1>Conferences</h1>
		</div>
	</div>

<div class="container" id="conferences" style="margin-top:21px;">

<% @conferences_month.each do |month, conferences| %>
	<div class="row">
		<div class="col"><h2><%= get_month(month) %><%= month %></h2></div>
	</div>
	<div class="row">
		<div class="col" style="padding-left: 28px;">
		<% for conference in conferences %>

			<ul class="list-unstyled">

				<li><%= link_to conference.conference, conference.link %></li>

				<% if !conference.venue.blank? %>
					<li><%= conference.venue %></li>
				<% end %>

				<li><%= conference.location %></li>

				<li><%= conference.dates %></li>

				<% if !conference.booth.blank? %>
					<li><%= conference.booth %></li>
				<% end %>

			</ul>
			<% end %>

		</div>
	</div>
<% end %>

</div>